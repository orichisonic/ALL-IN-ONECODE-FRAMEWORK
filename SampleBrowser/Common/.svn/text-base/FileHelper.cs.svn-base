using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace SampleBrowser.Common
{
    public class FileHelper
    {
        /// <summary>
        /// Get the terminally fold name from a path
        /// </summary>
        /// <param name="path"></param>
        /// <returns></returns>
        public static string GetDirectoryNameFromPath(string path)
        {
            return path.Substring(path.LastIndexOf("\\") + 1, 0);
        }
    }
}
